@model System.Collections.Generic.Dictionary<string,object>
@{
    ViewBag.Title = "Панель управления пользователя";
}
<h2 class="mb-3">Панель управления пользователя</h2>

<table class="table">
    @foreach (var pair in Model)
    {
        <tr>

            <td>@pair.Key</td>
            <td>@pair.Value</td>
        </tr>
    }
</table>
@Html.ActionLink("Редактировать", "Edit", "Account", null, new { @class = "btn btn-primary" })
@Html.ActionLink("Выйти", "Logout", "Account", null, new { @class = "btn btn-primary" })
@Html.ActionLink("Удалить", "Delete", "Account", null, new { @class = "delete btn btn-danger" })

